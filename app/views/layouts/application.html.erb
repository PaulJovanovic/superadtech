<!DOCTYPE html>
<html>
<head>
  <title>Adtech</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <script>
    (function() {
      var jovatech = window.jovatech || (window.jovatech = []);
      if (!jovatech.loaded) {
        var adjs = document.createElement('script');
        adjs.async = true;
        adjs.src = 'http://superadtech.herokuapp.com/v0/publishers/superjova/ad.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(adjs, s);
        jovatech.loaded = true;
      }
    })();
    window.jovatech = window.jovatech || [];
  </script>
</head>
<body style="margin: 0; padding: 0;font-family: Helvetica;color: #333333;">

<%= yield %>

</body>
</html>
